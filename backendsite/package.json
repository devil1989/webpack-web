{
  "name": "vue-demo",
  "version": "1.0.0",
  "description": "projects plugins setting",
  "author": "JeffreyChen",
  "private": true,
  "bin": {
    "p": "npm run p"
  },
  "scripts": {
    "stops": "taskkill /f /im node.exe",
    "webpack": "webpack --watch --config vue-demo/webpack.config.js",
    "debug": "set NODE_ENV=production&&node --inspect --debug-brk vue-demo/webpack.config.js",
    "start": "webpack-dev-server --progress --colors --inline --public --content-base vue-demo --config vue-demo/webpack.config.js",
    "test": "webpack --progress --colors --config vue-demo/webpack.config.js",

    "dev": "set NODE_ENV=dev&&webpack --progress --colors  --config vue-demo/webpack.config.js",
    "qa": "set NODE_ENV=qa&&webpack --progress --colors  --config vue-demo/webpack.config.js",
    "yz": "set NODE_ENV=yz&&webpack --progress --colors  --config vue-demo/webpack.config.js",
    "pro": "set NODE_ENV=production&&webpack --progress --colors  --config vue-demo/webpack.config.js",

    "buildLocal": "set NODE_ENV=dev&&webpack --progress --colors  --config vue-demo/webpack.config.dev.js",
    "copy": "cp -r ./vue-demo/build/assets/pages/* ../resources/templates/self-service/"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.0.0",
    "vue": "^2.1.0",
    "vue-router": "^2.1.1",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "acorn": "^5.0.3",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.1",
    "copy-webpack-plugin": "^4.0.1",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "es3ify-loader": "^0.2.0",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.7.1",
    "expose-loader": "^0.7.3",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "html-loader": "^0.4.5",
    "html-minify-loader": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "mustache": "^2.3.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "process": "^0.11.10",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.1",
    "webpack-dev-server": "^2.7.1"
  }
}
